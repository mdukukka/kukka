/** * @author Balakrishna [vamsibalu@gmail.com] * @version 2.0 */package com.events{		import flash.events.Event;
		public class AMFEvent extends Event{				public static const DEFAULT_NAME:String = "AMFEvent";		public static const FORKEY:String = "forkey"		public var _data:*;				public function AMFEvent($type:String,$data:*, $bubbles:Boolean = false, $cancelable:Boolean = false){			_data = $data;			super($type, $bubbles, $cancelable);		}				public override function clone():Event{			return new AMFEvent(type, _data, bubbles, cancelable);		}				public override function toString():String{			return formatToString("AMFEvent", "_data", "type", "bubbles", "cancelable");		}	}}